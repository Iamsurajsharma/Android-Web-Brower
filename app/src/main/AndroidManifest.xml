<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.radheshyam.mybrowser">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icblkhat"
        android:label="@string/app_name"

        android:roundIcon="@drawable/icblkhat"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
       >
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>\
        <activity android:name=".brows"
            android:label="Pass EditText"
            android:theme="@style/AppTheme">
        </activity>
        <activity android:name=".about"
            android:label="About Us"
            android:theme="@style/AppTheme">
        </activity>
        <activity android:name=".translator"
            android:label="Translator"
            android:theme="@style/AppTheme"/>

        <activity android:name=".history"
            android:label="History"
            android:theme="@style/AppTheme"/>
    </application>

</manifest>